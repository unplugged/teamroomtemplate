</lotusscript></code><code event='initialize'><lotusscript>Sub Initialize
	Dim sess As New NotesSession
	Dim db As NotesDatabase
	Dim doc As NotesDocument
	Dim i As Integer
	Dim max As Integer
	Dim words(20) As String
	Dim categories(4) As string
	Dim subject As string
	
	max = 1000
	categories(0) = "Advertising"
	categories(1) = "Design"
	categories(2) = "Development"
	categories(3) = "Manufacturing"
	categories(4) = "Sales"
	words(0)="icon"
	words(1)="idea"
	words(2)="song"
	words(3)="glee"
	words(4)="lime"
	words(5)="show"
	words(6)="sing"
	words(7)="next"
	words(8)="time"
	words(9)="left"
	words(10)="right"
	words(11)="math"
	words(12)="book"
	words(13)="epic"
	words(14)="find"
	words(15)="home"
	words(16)="full"
	words(17)="real"
	words(18)="bell"
	words(19)="make"
	words(20)="car"
	
	Set db = sess.Currentdatabase
	For i = 1 To max
		Set doc = db.Createdocument()
		Call doc.replaceItemValue("AltFrom", sess.Effectiveusername)
		Call doc.replaceItemValue("AIPriority", "3")
		Call doc.replaceItemValue("Categories", categories(3 * Rnd + 1))
		Call doc.replaceItemValue("DocType", "Discussion")
		Call doc.replaceItemValue("Form", "Document")
		Call doc.replaceItemValue("From", sess.Effectiveusername)
		subject = words(18 * Rnd + 1)
		subject = subject + " " + words(18 * Rnd + 1)
		subject = subject + " " + words(18 * Rnd + 1)
		subject = subject + " " + words(18 * Rnd + 1)
		Call doc.replaceItemValue("Subject", subject)
		Dim body As NotesRichTextItem
		Set body = doc.Createrichtextitem("Body")
		Call body.appendText(getBody())
	If i Mod 100 = 0 Then
		Call body.Embedobject(EMBED_ATTACHMENT, "", "c:\lon_con.pdf")
	ElseIf i Mod 50 = 0 Then
		Call body.Embedobject(EMBED_ATTACHMENT, "", "c:\This is a Word Document.docx")
	ElseIf i Mod 25 = 0 Then
		Call body.Embedobject(EMBED_ATTACHMENT, "", "c:\This is an Excel Sheet.xlsx")
	elseIf (i Mod 2 = 1) Then
		'Odd number
			Call body.Embedobject(EMBED_ATTACHMENT, "", "c:\This is a text file.txt")
			Call body.Embedobject(EMBED_ATTACHMENT, "", "c:\spinner_squares_circle.gif")
			Call body.Embedobject(EMBED_ATTACHMENT, "", "c:\ajax-loader.gif")		
	End If
		Call doc.Computewithform(False, false)
		Call doc.save(False, False)
	Next i
End Sub



